<!-- app/views/iqtests/new.html.erb -->
<h1>New IQ Test</h1>

<%= form_for @iqtest, url: iqtests_path do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <div>
    <%= f.label :price %>
    <%= f.text_field :price %>
  </div>

  <div>
    <%= f.submit "Create IQ Test" %>
  </div>



<% end %>
